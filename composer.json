  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.4",
    "gdbots/ncr": "^2.1",
    "triniti/schemas": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "triniti/acme-schemas": "^2.0"
  },
  "autoload": {
